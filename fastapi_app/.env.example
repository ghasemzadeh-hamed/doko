# Environment variables for the FastAPI sidecar service.
# The Django settings module must match the deployment target.
DJANGO_SETTINGS_MODULE=dokoplatform.settings.prod

# Uvicorn / service configuration
UVICORN_HOST=0.0.0.0
UVICORN_PORT=8001
UVICORN_RELOAD=False

# CORS configuration for FastAPI (comma separated list)
FASTAPI_ALLOWED_ORIGINS=https://frontend.example.com
